class Solution {
public:
    bool Find(int target, vector<vector<int> > array) {
        // 判断数组是否为空
        if (array.size() == 0 || array[0].size() == 0) return false;
        for ( auto& vec : array) {
            for ( int val : vec) {
                if (val == target)
                    return true;
            }
        }
        return false;
    }
};
