//跳台阶 记忆化搜索
class Solution {
public:
    int jumpFloor(int number) {
        vector<int> v(45,-1);
        return fib(number,v);
    }
    int fib(int n,vector<int> &v)
    {
        if(n<=1) return 1;
        else{
            if(v[n]!=-1) return v[n];
            else{
                v[n]=fib(n-1,v)+fib(n-2,v);
                return v[n];
            }
        }
    }
};
